{
  "name": "potlood",
  "version": "1.0.0",
  "description": "Potlood",
  "main": "build/main/index.js",
  "typings": "build/main/index.d.ts",
  "module": "build/module/index.js",
  "repository": "https://github.com/ynse01/potlood.git",
  "license": "MIT",
  "keywords": [],
  "scripts": {
    "info": "npm-scripts-info",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack --config webpack/dev.config.js --progress",
    "build:prod": "webpack --config webpack/prod.config.js --progress",
    "doc": "cpx \"dist/potlood.min.js\" ../potlood.github.io/lib -p",
    "reset": "git clean -dfx && git reset --hard && npm i",
    "clean": "trash build test"
  },
  "scripts-info": {
    "info": "Displays usage information of the package scripts",
    "build": "Clean and rebuild the project",
    "doc": "Generate HTML demo in docs folder",
    "reset": "Delete all untracked files and reset the repo to the last commit",
    "clean": "Cleans up all generated files"
  },
  "engines": {
    "node": ">=8.9"
  },
  "dependencies": {
    "jszip": "^3.2.2",
    "rtf.js": "^3.0.1",
    "utif": "^3.1.0"
  },
  "devDependencies": {
    "@types/jszip": "^3.1.6",
    "codecov": "^3.6.5",
    "cpx": "^1.5.0",
    "cz-conventional-changelog": "^2.1.0",
    "gh-pages": "^2.1.1",
    "npm-run-all": "^4.1.5",
    "npm-scripts-info": "^0.3.9",
    "nyc": "^13.1.0",
    "opn-cli": "^4.0.0",
    "prettier": "^1.19.1",
    "standard-version": "^4.4.0",
    "trash-cli": "^1.4.0",
    "ts-loader": "^7.0.2",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.17.0",
    "tslint-immutable": "^5.0.0",
    "typedoc": "^0.13.0",
    "typescript": "^3.7.4",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "ava": {
    "failFast": true,
    "files": [
      "build/main/**/*.spec.js"
    ],
    "sources": [
      "build/main/**/*.js"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "prettier": {
    "singleQuote": true
  },
  "nyc": {
    "exclude": [
      "**/*.spec.js"
    ]
  }
}
